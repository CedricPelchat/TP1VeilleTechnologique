      "permissions": [
        "NOTIFICATIONS"
      ],
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyCPBT9wpdOQcd_FklvW-MJ1R9LGhuebH-k"
        }
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
